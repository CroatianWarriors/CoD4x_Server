###############################################################################
# Common project properties.
MODULE=os_win32
CFLAGS=-c -g -O0
DEFINES=
LFLAGS=-shared
ADDITIONAL_OBJ=win_cod4.res
###############################################################################
# Arguments shortcut.
ARGS= MODULE="$(MODULE)" CFLAGS="$(CFLAGS)" DEFINES="$(DEFINES)" LFLAGS="$(LFLAGS)" ADDITIONAL_OBJ="$(ADDITIONAL_OBJ)"
###############################################################################
# Rules.
all:
	@$(MAKE) -s -f../../makefile_common $(ARGS)

clean:
	@$(MAKE) -s -f../../makefile_common clean $(ARGS)

clean_all:
	@$(MAKE) -s -f../../makefile_common clean_all $(ARGS)